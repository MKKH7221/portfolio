{"ast":null,"code":"import { ref } from 'vue';\nimport axios from \"axios\";\nexport default {\n  name: 'editUser',\n  props: {\n    id: String\n  },\n  data() {\n    return {\n      editUserForm: {\n        id: null,\n        name: null,\n        tel: null,\n        address: null,\n        country: {\n          name: null,\n          code: null\n        }\n      },\n      searchId: null,\n      countryList: [],\n      selectedCountry: {\n        name: \"\",\n        code: \"\"\n      }\n    };\n  },\n  created() {\n    this.searchId = this.$route.params.id;\n    this.getResults();\n    this.init();\n  },\n  methods: {\n    getResults() {\n      axios.get('http://localhost:8080/detail/' + this.searchId).then(response => {\n        this.editUserForm.id = response.data.id;\n        this.editUserForm.name = response.data.name;\n        this.editUserForm.address = response.data.address;\n        this.editUserForm.tel = response.data.tel;\n        this.editUserForm.country.name = response.data.country.name;\n        this.editUserForm.country.code = response.data.country.code;\n        alert(this.editUserForm.country.name + \"/\" + this.editUserForm.country.code);\n        this.selectedCountry.code = response.data.country.code;\n        this.selectedCountry.name = response.data.country.name;\n        alert(this.editUserForm.selectedCountry.name + \"/\" + this.selectedCountry.country.code);\n      }).catch(error => {\n        console.debug('error');\n        this.editUserForm.id = 2;\n        this.editUserForm.name = \"Jirou\";\n        console.log('error users: ' + this.editUserForm);\n      });\n    },\n    update() {\n      axios.post('http://localhost:8080/update', this.editUserForm).then(response => {\n        // this.editUserForm = response.data;\n        this.editUserForm.id = response.data.id;\n        this.editUserForm.name = response.data.name;\n        this.editUserForm.address = response.data.address;\n        this.editUserForm.tel = response.data.tel;\n        this.editUserForm.country.name = response.data.country.name;\n        this.editUserForm.country.code = response.data.country.code;\n        alert(\"Complete update\");\n      }).catch(error => {\n        if (error.response.status) {\n          this.message = `Fail to create user`;\n        } else {\n          this.message = `status: ${error.response.status}, message: ${error.response.data}`;\n        }\n      });\n    },\n    init() {\n      axios.get('http://localhost:8080/init').then(response => {\n        this.countryList = response.data;\n        alert(this, this.countryList.get(0));\n      }).catch(error => {\n        this.message = `init:status: ${error.response.status}, message: ${error.response.data}`;\n      });\n    }\n  }\n};","map":{"version":3,"names":["ref","axios","name","props","id","String","data","editUserForm","tel","address","country","code","searchId","countryList","selectedCountry","created","$route","params","getResults","init","methods","get","then","response","alert","catch","error","console","debug","log","update","post","status","message"],"sources":["/usr/src/app/src/pages/app/Edit.vue"],"sourcesContent":["<template>\n    <div class=\"content\" style=\"boeder:1px\">\n        <h3>Edit {{ this.$route.params.id }} </h3>    \n        <table class=\"table\">\n            <tr>\n                <th>Id</th>\n                <td><input v-model=\"editUserForm.id\" disabled=\"true\"></td>\n            </tr>\n            <tr>\n                <th>Name</th>\n                <td><input v-model=\"editUserForm.name\" /></td>\n            </tr>\n            <tr>\n                <th>Adress</th>\n                <td><input v-model=\"editUserForm.address\" /></td>\n            </tr>\n            <tr>\n                <th>Tel</th>\n                <td><input v-model=\"editUserForm.tel\" /></td>\n            </tr>\n            <tr>\n                <th>Country</th>\n                <!-- <td><input v-model=\"editUserForm.countryName\" /></td> -->\n           