{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      items: [],\n      message: \"\"\n    };\n  },\n  methods: {\n    search() {\n      axios.get('http://localhost:8080/search').then(response => {\n        for (let i = 0; i < response.data.length; i++) {\n          this.items.push(response.data[i]);\n        }\n      }).catch(error => {\n        console.debug('error');\n        let errorData1 = {\n          id: 2,\n          name: \"Jirou\"\n        };\n        let errorData2 = {\n          id: 3,\n          name: \"Saburou\"\n        };\n        this.items.push(errorData1);\n        this.items.push(errorData2);\n      });\n    },\n    deleteUser(paramId, paramName) {\n      console.log(paramId);\n      console.log(paramName);\n      const params = {\n        id: paramId\n      };\n      axios.delete('http://localhost:8080/delete', {\n        params\n      }).then(response => {\n        this.message = \"Successfully deleted user\" + paramName;\n        this.item = [];\n        console.log(this.item.length);\n        this.search();\n      }).catch(error => {\n        this.message = `status: ${error.response.status}, message: ${error.response.data}`;\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","items","message","methods","search","get","then","response","i","length","push","catch","error","console","debug","errorData1","id","name","errorData2","deleteUser","paramId","paramName","log","params","delete","item","status"],"sources":["/usr/src/app/src/pages/app/Search.vue"],"sourcesContent":["<template>\n    <div class=\"content\">\n        <h3>User List</h3>\n        <button @click=\"search\" type=\"button\" class=\"btn btn-primary\">Search</button>\n        <div v-show=\"message\" class=\"alert alert-danger\">{{message}}</div>\n        <table class=\"table\">\n            <thead>\n                <tr>\n                <th scope=\"col\">ID</th>\n                <th scope=\"col\">ユーザ名</th>\n                <th scope=\"col\"> </th>\n                <th scope=\"col\"> </th>\n                </tr>\n            </thead>\n            <tbody>   \n                <tr  v-for=\"item in items\" :key=\"item.message\">\n                    <th scope=\"row\">{{item.id}}</th>\n                    <td>{{item.name}}</td>\n                    <td>\n                        <button \n                            @click=\"() => $router.push({ name: 'Edit', params: {id: item.id }})\" \n                            type=\"button\"\n                            class=\"btn-primary\">Edit\n                        </button>\n                    </td>\n                    <td>    \n                        <button \n                            @click=\"deleteUser(item.id, item.name)\" \n                            type=\"button\"\n                            class=\"btn-primary\">Delete\n                        </button>\n                    </td>\n                </tr>   \n            </tbody>\n        </table>\n        <button @click=\"() => $router.push({ name: 'Add'})\"\n            type=\"button\" \n            class=\"btn btn-primary\"> User Add\n        </button>\n    </div>\n</template>\n<script>\n    import axios from 'axios';\n    export default {\n        data(){\n            return{\n                items: [],\n                message: \"\"\n            }\n        },\n        methods: {\n            search (){\n                axios\n                .get('http://localhost:8080/search')\n                .then(\n \n                    response => {\n                        for (let i=0; i<response.data.length; i++) {\n                            this.items.push(response.data[i]);\n                        }\n                    } \n                ).catch(\n                    error => {\n                        console.debug('error');\n                        let errorData1 = {id: 2, name: \"Jirou\"};\n                        let errorData2 = {id: 3, name: \"Saburou\"};\n                        this.items.push(errorData1);\n                        this.items.push(errorData2);\n                    }\n                );\n            },\n            deleteUser(paramId, paramName) {\n                console.log(paramId);\n                console.log(paramName);\n                const params = { id: paramId };\n                axios\n                .delete('http://localhost:8080/delete', { params })\n                .then(\n                    response => {\n                        this.message = \"Successfully deleted user\" + paramName;\n                        this.item = [];\n                        console.log(this.item.length);\n                        this.search();\n                    } \n                ).catch(\n                    error => {\n                        this.message = `status: ${error.response.status}, message: ${error.response.data}`;\n                    }\n                );\n            }\n        }\n    }\n</script>\n<style>\n.material-symbols-outlined {\n  font-variation-settings:\n  'FILL' 0,\n  'wght' 400,\n  'GRAD' 0,\n  'opsz' 24\n}\n</style>\n"],"mappings":";AA0CI,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACXC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;IACb;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,MAAKA,CAAA,EAAI;MACLL,KAAI,CACHM,GAAG,CAAC,8BAA8B,EAClCC,IAAI,CAEDC,QAAO,IAAK;QACR,KAAK,IAAIC,CAAC,GAAC,CAAC,EAAEA,CAAC,GAACD,QAAQ,CAACP,IAAI,CAACS,MAAM,EAAED,CAAC,EAAE,EAAE;UACvC,IAAI,CAACP,KAAK,CAACS,IAAI,CAACH,QAAQ,CAACP,IAAI,CAACQ,CAAC,CAAC,CAAC;QACrC;MACJ,CACJ,CAAC,CAACG,KAAK,CACHC,KAAI,IAAK;QACLC,OAAO,CAACC,KAAK,CAAC,OAAO,CAAC;QACtB,IAAIC,UAAS,GAAI;UAACC,EAAE,EAAE,CAAC;UAAEC,IAAI,EAAE;QAAO,CAAC;QACvC,IAAIC,UAAS,GAAI;UAACF,EAAE,EAAE,CAAC;UAAEC,IAAI,EAAE;QAAS,CAAC;QACzC,IAAI,CAAChB,KAAK,CAACS,IAAI,CAACK,UAAU,CAAC;QAC3B,IAAI,CAACd,KAAK,CAACS,IAAI,CAACQ,UAAU,CAAC;MAC/B,CACJ,CAAC;IACL,CAAC;IACDC,UAAUA,CAACC,OAAO,EAAEC,SAAS,EAAE;MAC3BR,OAAO,CAACS,GAAG,CAACF,OAAO,CAAC;MACp