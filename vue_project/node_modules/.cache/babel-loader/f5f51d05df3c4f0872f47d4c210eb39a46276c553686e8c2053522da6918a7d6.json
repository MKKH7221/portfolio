{"ast":null,"code":"import { ref } from 'vue';\nimport axios from \"axios\";\nexport default {\n  name: 'addUser',\n  data() {\n    return {\n      createUserForm: {\n        name: \"\",\n        address: \"\",\n        tel: \"\",\n        dept: \"\"\n      },\n      message: \"\",\n      countryList: []\n    };\n  },\n  created() {\n    this.init();\n  },\n  methods: {\n    addUser() {\n      axios.post(\"http://localhost:8080/add\", this.createUserForm).then(response => {\n        this.message = \"Created New User!\";\n        this.createUserForm.name = null;\n        this.createUserForm.address = null;\n        this.createUserForm.dept = null;\n        this.createUserForm.tel = null;\n      }).catch(error => {\n        if (error.response.status) {\n          this.message = `Fail to create user`;\n        } else {\n          this.message = `status: ${error.response.status}, message: ${error.response.data}`;\n        }\n      });\n    },\n    init() {\n      axios.get('http://localhost:8080/init').then(response => {\n        this.countryList = response.data;\n        alert(this, this.countryList.get(0));\n      }).catch(error => {\n        this.message = `init:status: ${error.response.status}, message: ${error.response.data}`;\n      });\n    }\n  }\n};","map":{"version":3,"names":["ref","axios","name","data","createUserForm","address","tel","dept","message","countryList","created","init","methods","addUser","post","then","response","catch","error","status","get","alert"],"sources":["/usr/src/app/src/pages/app/Add.vue"],"sourcesContent":["<template>\n    <div class=\"content\">\n        <h3>Create New User</h3>\n        <div class=\"card-body\">\n        <form v-on:submit.prevent=\"addUser\">\n          <div v-show=\"message\" class=\"alert alert-danger\">{{message}}</div>\n          <div class=\"form-group\">\n            <label>User Name:</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"createUserForm.name\" />\n          </div>\n       