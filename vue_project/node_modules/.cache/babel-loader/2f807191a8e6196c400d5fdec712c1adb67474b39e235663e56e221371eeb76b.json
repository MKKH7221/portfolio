dev>    \n        <p class=\"button_align\">\n            <button @click=\"search\" type=\"button\" class=\"btn btn-primary\">Search</button>\n        </p>\n        <p>id: {{ consId }} countryCode: {{ countryCode }} name: {{ name }} address: {{ address }} tel:{{ tel }}</p>\n        <hr>\n        <!-- for message -->\n        <div v-show=\"message\" class=\"alert alert-danger\">{{message}}</div>\n        <!-- user List -->\n        <table class=\"table\">\n            <thead>\n                <tr>\n                <th scope=\"col\">ID</th>\n                <th scope=\"col\">Name</th>\n                <th scope=\"col\">Address</th>\n                <th scope=\"col\">Tel</th>\n                <th scope=\"col\">Country</th>\n                <th scope=\"col\">&nbsp;</th>\n                <th scope=\"col\">&nbsp;</th>\n                </tr>\n            </thead>\n            <tbody>   \n                <tr v-for=\"user in users\" :key=\"user.id\">\n                    <th scope=\"row\">{{user.id}}</th>\n                    <td>{{user.name}}</td>\n                    <td>{{user.address}}</td>   \n                    <td>{{user.tel}}</td>\n                    <td>{{user.country.name}}</td>\n                    <td>\n                        <button @click=\"() => $router.push({ name: 'Edit', params: {id: user.id }})\" \n                            type=\"button\" class=\"btn btn-primary\">Edit\n                        </button>\n                    </td>\n                    <td>    \n                        <button @click=\"deleteUser(user.id, user.name)\" type=\"button\" \n                        class=\"btn btn-primary\">Delete</button>\n                    </td>\n                </tr>   \n            </tbody>\n        </table>\n        <span>\n            <button @click=\"()=>$router.push({ name: 'Add'})\" type=\"button\" class=\"btn btn-primary\"> User Add </button>\n        </span>\n    </div>\n</template>\n<script>\n    import axios from 'axios';\n    export default {\n        data(){\n            return{\n                users: [],\n                message: \"\",\n                countryList: [],\n                // conditions\n                consId :\"\",\n                countryCode: \"\",\n                name: \"\",\n                address: \"\",\n                tel: \"\"\n            }\n        },\n        mounted(){\n            axios.get('http://localhost:8080/init')\n                .then (\n                    response => {this.countryList = response.data;} \n                ).catch (\n                    error => {\n                        this.message = `init:status: ${error.response.status}, message: ${error.response.data}`;\n                    }\n                ); \n        },\n        methods: {\n            search (){\n                alert(\"condition: id \" + this.consId\n                    + \" name\" + this.name\n                    + \" address\" + this.address\n                    + \" tel\" + this.tel\n                    + \" country\" + this.countryCode\n                    );\n\n                    let params = new FormData()\n                    params.append('id', searchConditions.consId)\n                    params.append('name', this.searchConditions.name)\n                    params.append('address',this.searchConditions.address)\n                    params.append('tel',this.searchConditions.tel) \n                    params.append('countryCode',this.searchConditions.countryCode) \n\n                axios\n                .get('http://localhost:8080/search', params)\n                .then(\n                    response => {\n                        this.users = response.data;\n                        // for (let i=0; i<response.data.length; i++) {\n                        //     this.users.push(response.data[i]);\n                        // }\n                    } \n                ).catch(\n                    error => {\n                        this.message = `status: ${error.response.status}, message: ${error.response.data}`;\n                    }\n                );\n            },\n            deleteUser(paramId, paramName) {\n                console.log(paramId);\n                console.log(paramName);\n                const params = { id: paramId };\n                axios\n                .delete('http://localhost:8080/delete', { params })\n                .then(\n                    response => {\n                        this.message = \"Successfully deleted user\" + paramName;\n                        this.search();\n                    } \n                ).catch(\n                    error => {\n                        this.message = `status: ${error.response.status}, message: ${error.response.data}`;\n                    }\n                );\n            }\n        }\n    }\n</script>\n<style>\n.material-symbols-outlined {\n  font-variation-settings:\n  'FILL' 0,\n  'wght' 400,\n  'GRAD' 0,\n  'opsz' 24\n}\n\n.condition {\n  max-width: 75%;\n  margin: 40px auto;\n  padding: 20px;\n  align-self: auto;\n}\n\n.condition th {\n    background-color: lavenderblush;\n    width: 15%;\n    text-align: center;\n}\n.button_align {\n\ttext-align: right;\n}\n\n</style>\n"],"mappings":"AAoFI,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACXC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,EAAE;MACXC,WAAW,EAAE,EAAE;MACf;MACAC,MAAK,EAAG,EAAE;MACVC,WAAW,EAAE,EAAE;MACfC,IAAI,EAAE,EAAE;MACRC,OAAO,EAAE,EAAE;MACXC,GAAG,EAAE;IACT;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAE;IACLV,KAAK,CAACW,GAAG,CAAC,4BAA4B,EACjCC,IAAG,CACAC,QAAO,IAAK;MAAC,IAAI,CAACT,WAAU,GAAIS,QAAQ,CAACZ,IAAI;IAAC,CAClD,CAAC,CAACa,KAAI,CACFC,KAAI,IAAK;MACL,IAAI,CAACZ,OAAM,GAAK,gBAAeY,KAAK,CAACF,QAAQ,CAACG,MAAO,cAAaD,KAAK,CAACF,QAAQ,CAACZ,IAAK,EAAC;IAC3F,CACJ,CAAC;EACT,CAAC;EACDgB,OAAO,EAAE;IACLC,MAAKA,CAAA,EAAI;MACLC,KAAK,CAAC,gBAAe,GAAI,IAAI,CAACd,MAAK,GAC7B,OAAM,GAAI,IAAI,CAACE,IAAG,GAClB,UAAS,GAAI,IAAI,CAACC,OAAM,GACxB,MAAK,GAAI,IAAI,CAACC,GAAE,GAChB,UAAS,GAAI,IAAI,CAACH,WACpB,CAAC;MAED,IAAIc,MAAK,G