{"ast":null,"code":"import { ref } from 'vue';\nimport axios from \"axios\";\nexport default {\n  name: 'editUser',\n  props: {\n    id: String\n  },\n  data() {\n    return {\n      editUserForm: {\n        id: null,\n        name: null,\n        tel: null,\n        address: null,\n        countryName: null,\n        countryCode: null\n      },\n      searchId: null,\n      countryList: []\n    };\n  },\n  created() {\n    this.searchId = this.$route.params.id;\n    this.getResults();\n  },\n  methods: {\n    getResults() {\n      axios.get('http://localhost:8080/detail/' + this.searchId).then(response => {\n        this.editUserForm.id = response.data.id;\n        this.editUserForm.name = response.data.name;\n        this.editUserForm.address = response.data.address;\n        this.editUserForm.tel = response.data.tel;\n        this.editUserForm.countryName = response.data.country.name;\n        this.editUserForm.countryCode = response.data.country.code;\n      }).catch(error => {\n        console.debug('error');\n        this.editUserForm.id = 2;\n        this.editUserForm.name = \"Jirou\";\n        console.log('error users: ' + this.editUserForm);\n      });\n    },\n    update() {\n      axios.post('http://localhost:8080/update', this.editUserForm).then(response => {\n        this.editUserForm = response.data;\n        // this.editUserForm.id = response.data.id;\n        // this.editUserForm.name = response.data.name;\n        // this.editUserForm.dept = response.data.dept;\n        // this.editUserForm.address = response.data.address;\n        // this.editUserForm.tel = response.data.tel;\n        alert(\"Complete update\");\n      }).catch(error => {\n        if (error.response.status) {\n          this.message = `Fail to create user`;\n        } else {\n          this.message = `status: ${error.response.status}, message: ${error.response.data}`;\n        }\n      });\n    },\n    init() {\n      axios.get('http://localhost:8080/init').then(response => {\n    