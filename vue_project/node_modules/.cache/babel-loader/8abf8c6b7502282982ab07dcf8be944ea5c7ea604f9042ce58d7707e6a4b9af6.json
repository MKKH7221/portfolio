  <label>Address:</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"user.address\" />\n          </div>\n          <div class=\"form-group\">\n            <input type=\"submit\" class=\"btn btn-primary\" value=\"Add User\" />\n          </div>\n        </form>\n      </div>\n        \n    </div>\n</template>\n  \n  <script>\n  import { ref } from 'vue';\n  import axios from \"axios\";\n  export default {\n    name: 'addUser',\n    data(){\n        return{\n          user: {\n            name: \"\",\n            address: \"\"\n          },   \n          message : \"\",\n        }\n    },\n    methods: {\n        addUser() {\n\n          alert('Do you want to add this user? : ' + this.user.name);\n          console.log(this.user.name);\n          console.log(this.user.address);\n          \n          const createUserForm = new FormData();\n          for (let key in this.data) {\n            formData.append(key, data[key]);\n          }\n\n        //   formData.append(\"name\", this.user.name);\n        //   formData.append(\"address\", this.user.address);\n\n          axios\n          .post(\"http://localhost:8080/add\", \n          createUserForm,\n            // add\n            {headers: {'Content-Type': 'multipart/form-data'}}\n            // { headers: {'Content-Type':'application/json'} } //Content-Typeを記述\n          )\n          .then(\n            response => {\n              \n              this.message = \"Created Added New User!\";\n              this.user.id = null;\n              this.user.name = null;\n            } \n          ).catch(\n            error => {\n              this.message = `status: ${error.response.status}, message: ${error.response.data}`;\n            }\n          );    \n        }\n    }\n    \n  }\n  </script>\n  \n  <style>\n  /*CSSを記載*/\n  </style>\n  "],"mappings":"AA0BE,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAE;IACF,OAAM;MACJC,IAAI,EAAE;QACJF,IAA