{"ast":null,"code":"import { ref } from 'vue';\nimport axios from \"axios\";\nexport default {\n  name: 'addUser',\n  data() {\n    return {\n      message: \"\"\n    };\n  },\n  methods: {\n    addUser() {\n      alert('Do you want to add this user? : ' + this.user.name);\n      const formData = new FormData();\n      formData.append(\"name\", this.user.name);\n      formData.append(\"address\", this.user.address);\n      console.log(formData.name);\n      console.log(formData.address);\n      this.axios.post(\"http://localhost:8080/add\", formData, {\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      } //Content-Typeを記述\n      ).then(response => {\n        this.$swal({\n          icon: \"success\",\n          text: \"Created Added New User!\"\n        });\n        this.user.id = null;\n        this.user.name = null;\n      }).catch(error => {\n        this.message = `status: ${error.response.status}, message: ${error.response.data}`;\n      });\n    }\n  }\n};","map":{"version":3,"names":["ref","axios","name","data","message","methods","addUser","alert","user","formData","FormData","append","address","console","log","post","headers","then","response","$swal","icon","text","id","catch","error","status"],"sources":["/usr/src/app/src/pages/app/Add.vue"],"sourcesContent":["<template>\n    <div class=\"content\">\n        <h3>Create New User</h3>\n        <div class=\"card-body\">\n        <form v-on:submit.prevent=\"addUser\">\n\n          <div v-show=\"message\" class=\"alert alert-danger\">{{message}}</div>\n          <div class=\"form-group\">\n            <label>User Name:</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"user.name\" />\n          </div>\n\n          <div class=\"form-group\">\n            <label>Address:</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"user.address\" />\n          </div>\n          <div class=\"form-group\">\n            <input type=\"submit\" class=\"btn btn-primary\" value=\"Add User\" />\n          </div>\n        </form>\n      </div>\n        \n    </div>\n</template>\n  \n  <script>\n  import { ref } from 'vue';\n  import axios from \"axios\";\n  export default {\n    name: 'addUser',\n    data(){\n        return{\n            message : \"\"\n        }\n    },\n    methods: {\n        addUser() {\n          alert('Do you want to add this user? : ' + this.user.name);\n          const formData = new FormData();\n          formData.append(\"name\", this.user.name);\n          formData.append(\"address\", this.user.address);\n          console.log(formData.name);\n          console.log(formData.address);\n\n          this.axios\n          .post(\"http://localhost:8080/add\", formData, { headers: {'Content-Type':'application/json'} } //Content-Typeを記述\n          )\n          .then(\n            response => {\n              this.$swal({\n                icon: \"success\",\n                text: \"Created Added New User!\"\n              });\n              this.user.id = null;\n              this.user.name = null;\n            } \n          ).catch(\n            error => {\n              this.message = `status: ${error.response.status}, message: ${error.response.data}`;\n            }\n          );    \n        }\n    }\n    \n  }\n  </script>\n  \n  <style>\n  /*CSSを記載*/\n  </style>\n  "],"mappings":"AA0BE,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,OAAM,EAAI;IACd;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,OAAOA,CAAA,EAAG;MACRC,KAAK,CAAC,kCAAiC,GAAI,IAAI,CAACC,IAAI,CAACN,IAAI,CAAC;MAC1D,MAAMO,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/BD,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACH,IAAI,CAACN,IAAI,CAAC;MACvCO,QAAQ,CAACE,MAAM,CAAC,SAAS,EAAE,IAAI,CAACH,IAAI,CAACI,OAAO,CAAC;MAC7CC,OAAO,CAACC,GAAG,CAACL,QAAQ,CAACP,IAAI,CAAC;MAC1BW,OAAO,CAACC,GAAG,CAACL,QAAQ,CAACG,OAAO,CAAC;MAE7B,IAAI,CAACX,KAAI,CACRc,IAAI,CAAC,2BAA2B,EAAEN,QAAQ,EAAE;QAAEO,OAAO,EAAE;UAAC,cAAc,EA