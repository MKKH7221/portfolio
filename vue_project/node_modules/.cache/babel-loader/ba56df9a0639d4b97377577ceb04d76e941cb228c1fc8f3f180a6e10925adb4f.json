 -->\n        <dev class=\"condition\" >\n            <hr>\n            <h4> search condition</h4>\n            <form>\n                <table>\n                    <tr>\n                        <th id=\"th_condition\" >id</th>\n                        <td>\n                            <input type=\"text\" v-model=\"consId\">\n                        </td>\n                        <th id=\"th_condition\" >User Name</th>\n                        <td>\n                            <input type=\"text\" v-model=\"name\">\n                        </td>\n                        <th id=\"th_condition\">Address</th>\n                        <td><input type=\"text\" v-model=\"address\"></td>\n                    </tr>\n                    <tr>\n                        <th id=\"th_condition\">Tel</th>\n                        <td>\n                            <input type=\"text\" v-model=\"tel\">\n                        </td>\n                        <th id=\"th_condition\">Country</th>\n                        <td width=\"30px\">\n                            <select v-model=\"countryCode\" id=\"selectedCountryCode\">\n                                <option v-for=\"country in countryList\" v-bind:key=\"country.name\" v-bind:value=\"country.code\">\n                                    {{ country.name }}\n                                </option>\n                            </select>\n                        </td>\n                        <th>&nbsp;</th>\n                        <td>&nbsp;</td>\n                    </tr>\n                </table>\n            </form>\n        </dev>    \n        <p class=\"button_align\">\n            <button @click=\"search\" type=\"button\" class=\"btn btn-primary\">Search</button>\n        </p>\n        <p>id: {{ consId }} countryCode: {{ countryCode }} name: {{ name }} address: {{ address }} tel:{{ tel }}</p>\n        <hr>\n        <!-- for message -->\n        <div v-show=\"message\" class=\"alert alert-danger\">{{message}}</div>\n        <!-- user List -->\n        <table class=\"table\">\n            <thead>\n                <tr>\n                <th scope=\"col\">ID</th>\n                <th scope=\"col\">Name</th>\n                <th scope=\"col\">Address</th>\n                <th scope=\"col\">Tel</th>\n                <th scope=\"col\">Country</th>\n                <th scope=\"col\">&nbsp;</th>\n                <th scope=\"col\">&nbsp;</th>\n                </tr>\n            </thead>\n            <tbody>   \n                <tr v-for=\"user in users\" :key=\"user.id\">\n                    <th scope=\"row\">{{user.id}}</th>\n                    <td>{{user.name}}</td>\n                    <td>{{user.address}}</td>   \n                    <td>{{user.tel}}</td>\n                    <td>{{user.country.name}}</td>\n                    <td>\n                        <button @click=\"() => $router.push({ name: 'Edit', params: {id: user.id }})\" \n                            type=\"button\" class=\"btn btn-primary\">Edit\n                        </button>\n                    </td>\n                    <td>    \n                        <button @click=\"deleteUser(user.id, user.name)\" type=\"button\" \n                        class=\"btn btn-primary\">Delete</button>\n                    </td>\n                </tr>   \n            </tbody>\n        </table>\n        <span>\n            <button @click=\"()=>$router.push({ name: 'Add'})\" type=\"button\" class=\"btn btn-primary\"> User Add </button>\n        </span>\n    </div>\n</template>\n<script>\n    import axios from 'axios';\n    export default {\n        data(){\n            return{\n                users: [],\n                message: \"\",\n                countryList: [],\n                // conditions\n                consId :\"\",\n                countryCode: \"\",\n                name: \"\",\n                address: \"\",\n                tel: \"\"\n            }\n        },\n        mounted(){\n            axios.get('http://localhost:8080/init')\n                .then (\n                    response => {this.countryList = response.data;} \n                ).catch (\n                    error => {\n                        this.message = `init:status: ${error.response.status}, message: ${error.response.data}`;\n                    }\n                ); \n        },\n        methods: {\n            search (){\n                axios\n                .get('http://localhost:8080/search', { \n                    params: {\n                        id : this.consId,\n                        name : this.name,\n                        address : this.address,\n                        tel : this.tel,\n                        countryCode : this.countryCode\n                    }\n                }).then(\n                    response => {\n                        this.users = response.data;\n                    } \n                ).catch(\n                    error => {\n                        this.message = `status: ${error.response.status}, message: ${error.response.data}`;\n                    }\n                );\n            },\n            deleteUser(paramId, paramName) {\n                console.log(paramId);\n                console.log(paramName);\n                const params = { id: paramId };\n                axios\n                .delete('http://localhost:8080/delete/', { params })\n                // .delete(`http://localhost:8080/delete/${paramId}`)\n                .then(\n                    response => {\n                        this.message = \"Successfully deleted user\" + paramName;\n                        this.search();\n                    } \n                ).catch(\n                    error => {\n                        this.message = `status: ${error.response.status}, message: ${error.response.data}`;\n                    }\n                );\n            }\n        }\n    }\n</script>\n<style>\n.material-symbols-outlined {\n  font-variation-settings:\n  'FILL' 0,\n  'wght'