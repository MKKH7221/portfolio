{"ast":null,"code":"import { ref } from 'vue';\nimport axios from \"axios\";\nexport default {\n  name: 'detail',\n  props: {\n    id: String\n  },\n  data() {\n    return {\n      user: {\n        id: null,\n        name: null,\n        tel: null,\n        address: null,\n        dept: null\n      },\n      searchId: null\n    };\n  },\n  created() {\n    this.searchId = this.$route.params.id;\n    console.log('params what we get is === ' + this.searchId);\n    this.getResults(this.searchId);\n  },\n  methods: {\n    getResults(q) {\n      axios.get('http://localhost:8080/detail', {\n        params: {\n          id: q\n        }\n      }).then(response => {\n        this.user.id = response.data.id;\n        this.user.name = response.data.name;\n        this.user.dept = response.data.dept;\n        this.user.address = response.data.address;\n        this.user.tel = response.data.tel;\n      }).catch(error => {\n        console.debug('error');\n        this.user.id = 2;\n        this.user.name = \"Jirou\";\n        console.log('error users: ' + this.user);\n      });\n    },\n    update() {\n      axios.post('http://localhost:8080/update', this.user).then(response => {\n        this.user.id = response.data.id;\n        this.user.name = response.data.name;\n        this.user.dept = response.data.dept;\n        this.user.address = response.data.address;\n        this.user.tel = response.data.tel;\n        alert(\"登録が完了しました\");\n      }).catch(error => {\n        alert(error);\n        console.debug('error');\n        this.user.id = 2;\n        this.user.name = \"Jirou\";\n        console.log('error users: ' + this.user.name);\n      });\n    }\n  }\n};","map":{"version":3,"names":["ref","axios","name","props","id","String","data","user","tel","address","dept","searchId","created","$route","params","console","log","getResults","methods","q","get","then","response","catch","error","debug","update","post","alert"],"sources":["/usr/src/app/src/pages/app/Edit.vue"],"sourcesContent":["<template>\n    <div class=\"content\">\n        <h3>Edit {{ this.$route.params.id }} </h3>    \n        <table class=\"table\">\n            <tr>\n                <th>Id　</th>\n                <td><input v-model=\"user.id\" disabled=\"true\"></td>\n            </tr>\n            <tr>\n                <th>Name　</th>\n                <td><input v-model=\"user.name\" /></td>\n            </tr>\n            <tr>\n                <th>Dept　</th>\n                <td><input v-model=\"user.dept\" /></td>\n            </tr>\n            <tr>\n                <th>Adress　</th>\n                <td><input v-model=\"user.address\" /></td>\n            </tr>\n            <tr>\n                <th>Tel　</th>\n                <td><input v-model=\"user.tel\" /></td>\n            </tr>\n        </table>\n        <button @click=\"update()\" type=\"button\" class=\"btn-primary\"> Confirm </button>\n    </div>\n</template>\n  \n  <script>\n  import { ref } from 'vue';\n  import axios from \"axios\";\n  export default {\n    name: 'detail',\n    props: {\n      id: String\n    },\n    data(){\n      return{\n        user: {\n            id: null,\n            name: null,\n            tel: null,\n            address: null,\n            dept: null\n        },\n        searchId: null\n      }\n    },\n    created() {\n        this.searchId = this.$route.params.id;\n        console.log('params what we get is === ' + this.searchId);\n        this.getResults(this.searchId);\n    },\n    methods: {\n        getResults(q) {\n            axios\n            .get('http://localhost:8080/detail', {params:  {id: q}})\n                .then(\n                    response => {\n                        this.user.id = response.data.id;\n                        this.user.name = response.data.name;\n                        this.user.dept = response.data.dept;\n                        this.user.address = response.data.address;\n                        this.user.tel = response.data.tel;\n                    } \n                ).catch(\n                    error => {\n                        console.debug('error');\n                        this.user.id = 2;\n                        this.user.name = \"Jirou\";\n                        console.log('error users: ' + this.user);\n                    }\n                );    \n        },\n        update() {\n            axios.post('http://localhost:8080/update',this.user)\n            .then(\n                response => {\n                    this.user.id = response.data.id;\n                    this.user.name = response.data.name;\n                    this.user.dept = response.data.dept;\n                    this.user.address = response.data.address;\n                    this.user.tel = response.data.tel;\n                    alert(\"登録が完了しました\");\n                } \n            ).catch(\n                error => {\n                    alert(error);\n                    console.debug('error');\n                    this.user.id = 2;\n                    this.user.name = \"Jirou\";\n                    console.log('error users: ' + this.user.name);\n                }\n            );    \n        },\n    }\n  }\n  </script>\n  \n  <!-- <style>\n  table, th, td {\n    border: 1px solid black;\n    border-collapse: collapse;\n  }\n  </style> -->\n  "],"mappings":"AA8BE,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,KAAK,EAAE;IACLC,EAAE,EAAEC;EACN,CAAC;EACDC,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,IAAI,EAAE;QACFH,EAAE,EAAE,IAAI;QACRF,IAAI,EAAE,IAAI;QACVM,GAAG,EAAE,IAAI;QACTC,OAAO,EAAE,IAAI;QACbC,IAAI,EAAE;MACV,CAAC;MACDC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACD,QAAO,GAAI,IAAI,CAACE,MAAM,CAACC,MAAM,CAACV,EAAE;IACrCW,OAAO,CAACC,GAAG,CAAC,4BAA2B,GAAI,IAAI,CAACL,QAAQ,CAAC;IACzD,IAAI,CAACM,UAAU,CAAC,IAAI,CAACN,QAAQ,CAAC;EAClC,CAAC;EACDO,OAAO,EAAE;IACLD,UAAUA,CAACE,CAAC,EAAE;MACVlB,KAAI,CACHmB,GAAG,CAAC,8BAA8B,EAAE;QAACN,MAAM,EAAG;UAACV,EAAE,EAAEe;QAAC;MAAC,CAAC,EAClDE,IA