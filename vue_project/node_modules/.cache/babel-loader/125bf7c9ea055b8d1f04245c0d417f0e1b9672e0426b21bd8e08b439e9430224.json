ess:</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"createUserForm.address\" />\n          </div>\n\n          <div class=\"form-group\">\n            <input type=\"submit\" class=\"btn btn-primary\" value=\"Add User\" />\n          </div>\n        </form>\n      </div>\n        \n    </div>\n</template>\n  \n  <script>\n  import { ref } from 'vue';\n  import axios from \"axios\";\n  export default {\n    name: 'addUser',\n    data(){\n        return{\n          createUserForm: {\n            name: \"\",\n            address: \"\",\n            tel: \"\",\n            dept: \"\"\n          },   \n          message : \"\",\n        }\n    },\n    methods: {\n        addUser() {\n          axios\n          .post(\"http://localhost:8080/add\", this.createUserForm)\n          .then(\n            response => {\n              this.message = \"Created New User!\";\n              this.createUserForm.name = null;\n              this.createUserForm.address = null;\n              this.createUserForm.dept = null;\n              this.createUserForm.tel = null;\n            } \n          ).catch(\n            error => {\n                if (error.response.status) {\n                    this.message = `Fail to create user`;\n                } else {\n                    this.message = `status: ${error.response.status}, message: ${error.response.data}`;\n                }\n            }\n          );    \n        }\n    }\n    \n  }\n  </script>\n  \n  <style>\n  /*CSSを記載*/\n  </style>\n  "],"mappings":"AAoCE,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAE;IACF,OAAM;MACJC,cAAc,EAAE;QACdF,IAAI,EAAE,EAAE;QACRG,OAAO,EAAE,EAAE;QACXC,GAAG,EAAE,EAAE;QACPC,IAAI,EAAE;MACR,CAAC;MACDC,OAAM,EAAI;IACZ;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,OAAOA,CAAA,EAAG;MACRT,KAAI,CACHU,IAAI,CAAC,2BAA2B,EAAE,IAAI,CAACP,cAAc,EACrDQ,IAAI,CACHC,QAAO,IAAK;QACV,IAAI,CAACL,OAAM,GAAI,mBAAmB;QAClC,IAAI,CAACJ,cAAc,CAACF,IAAG,GAAI,IAAI;QAC/B,IA